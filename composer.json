{
    "name": "rawphp/raw-options",
    "type": "library",
    "description": "RawOptions is a small package that adds options to a PHP application and is used by RawPHP framework and other applications.",
    "keywords": [ "options" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "rawphp/raw-base": "1.0.0-RC-1",
        "rawphp/raw-migrator": "0.*@dev",
        "rawphp/raw-database": "0.*@dev",
        "rawphp/raw-yaml": "1.0.0-RC-2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "squizlabs/php_codesniffer": "dev-master",
        "rawphp/raw-code-standards": "1.*@dev",
        "phpdocumentor/phpdocumentor": "dev-master",
		"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "classmap": [
            "lib/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
